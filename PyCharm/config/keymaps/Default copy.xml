<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateDebugToolWindow">
    <keyboard-shortcut first-keystroke="alt 5" />
    <keyboard-shortcut first-keystroke="alt f5" />
  </action>
  <action id="CodeCompletion">
    <keyboard-shortcut first-keystroke="ctrl space" />
    <keyboard-shortcut first-keystroke="back_quote" />
  </action>
  <action id="CopyElement" />
  <action id="EditorDecreaseFontSize">
    <mouse-shortcut keystroke="control button142" />
  </action>
  <action id="EditorIncreaseFontSize">
    <mouse-shortcut keystroke="control button143" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
    <keyboard-shortcut first-keystroke="f5" />
  </action>
</keymap>